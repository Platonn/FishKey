        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/FishKeyTheme" >
        
        <activity android:name="com.fishkey.QuizActivity" android:label="@string/app_name"> <!-- TODO: Potem zmieniÄ‡ na aktywnosc glowna -->
	      <intent-filter>
	        <action android:name="android.intent.action.MAIN" />
	        <category android:name="android.intent.category.LAUNCHER" />
	      </intent-filter>
	    </activity>
    </application>

</manifest>